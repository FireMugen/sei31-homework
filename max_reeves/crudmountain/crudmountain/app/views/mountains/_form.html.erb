<%= form_for @mountains do |f| %>
	<%= f.label :name, :autofocus => true, :required => true  %>
	<%= f.text_field :name, :placeholder => 'eg. Mt Everest'%>
	<%= f.label :country %>
	<%= f.text_field :country %>
	<%= f.label :image %>
	<%= f.url_field :image, :placeholder => 'Insert IMG URL!' %>
	<%= f.label :height %>
	<%= f.text_field :height %>
	<%= f.submit  %>
<% end %>
